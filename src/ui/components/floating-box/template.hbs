{{#paper-card as |card|}}
  {{#card.title as |title|}}
    {{#title.text as |text|}}
    {{#text.headline}}{{{this.card.title}}}{{/text.headline}}
    {{/title.text}}
  {{/card.title}}
  {{#card.content class="layout-row layout-align-space-between"}}
    {{yield}}
  {{/card.content}}
{{/paper-card}}
