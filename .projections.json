{
  "src/services/*.js": {
    "type": "service",
    "alternate": "src/services/{}-test.js"
  },
  "src/ui/routes/*/template.hbs": {
    "type": "rtemplate",
    "alternate": "src/ui/routes/{}/route-test.js"
  },
  "src/ui/routes/*/route-test.js": {
    "type": "rtest",
    "alternate": "src/ui/routes/{}/route.js"
  },
  "src/ui/routes/*/route.js": {
    "type": "route",
    "alternate": "src/ui/routes/{}/template.hbs"
  },
  "src/ui/components/*/template.hbs": {
    "type": "ctemplate",
    "alternate": "src/ui/components/{}/component-test.js"
  },
  "src/ui/components/*/component-test.js": {
    "type": "ctest",
    "alternate": "src/ui/components/{}/component.js"
  },
  "src/ui/components/*/component.js": {
    "type": "component",
    "alternate": "src/ui/components/{}/template.hbs"
  },
  "src/ui/styles/*.scss": {
    "type": "sass"
  }
}
