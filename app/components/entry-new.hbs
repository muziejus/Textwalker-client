<div class="flex flex-col p-2 shadow-md border mb-2">
    <EmberTable as |t|>
      <t.head @columns={{this.columns}} as |h|>
        <h.row as |r|>
          <r.cell as |column|>
            {{#component "table-column-header" columnContent=column}}
              {{column.name}}
            {{/component}}
          </r.cell>
        </h.row>
      </t.head>
      <t.body @rows={{this.rows}} as |b|>
        <b.row as |r|>
          <r.cell as |cell column entry|>
            <TableCell
              @entry={{entry}}
              @column={{column}}
              @focusIn={{action this.focusIn}}
            />
          </r.cell>
        </b.row>
      </t.body>
    </EmberTable>

    <Button @onClick={{this.createEntry}} @classes="mt-4">
      Save that, frend
    </Button>


</div>
