{{#if this.model.isRunning}}
  <p>Spinner!</p>
{{else}}
  <h2>{{text-title this.model }}</h2>
  <div>
    {{#if this.model.markdownBlurb }}
      {{markdown-to-html this.model.markdownBlurb }}
    {{else}}
      _non_ markdown blurb
    {{/if}}
  </div>
  <div class="my-4 flex flex-row">
    <div>
      {{#link-to "workbench.texts.text.entries" this.model }}Entries{{/link-to}}
    </div>
    <div>
      Some other button
    </div>
  </div>
  {{outlet}}
{{/if}}
