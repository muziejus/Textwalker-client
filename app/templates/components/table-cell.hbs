<td class={{ this.classes }} style={{this.cellWidth}}>
  {{#if this.isPlace}}
    <div class="flex flex-row">
      {{#if this.fetchPlace.isRunning}}
        <div>Thinkingâ€¦</div>
        <div class="ml-auto">
          {{svg-jar "circle-o-notch" class="text-primary-500 fill-current rotate"}}
        </div>
      {{else}}
        <div>{{this.place.name}}</div>
        {{#if this.isMappable}}
          <button class="block press-play px-1 ml-auto" 
            onClick={{action @showModal this.place}} >
            {{svg-jar "map-marker" class="fill-current text-primary-500" }} 
          </button>
        {{/if}}
      {{/if}}
    </div>
  {{else if this.isDate}}
    {{moment-format (get @changeset this.property) "YYYY-MM-DD"}}
  {{else}}
    {{input
      type=this.inputType
      value=(get @changeset this.property)
      class=(concat "inline-editor table-edit-input entry-property-" this.property " entry-id-" (get @changeset "id") " " this.validationClass)
      disabled=this.isDisabled
      focus-out=(action this.validate)
      focus-in=(action @focusIn)
      key-up=(queue (action this.validate) (action (changeset-set @changeset this.property) value=target.value))
    }}
  {{/if}}
</td>
